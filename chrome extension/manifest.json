{
    "manifest_version": 3,
    "name": "Space Face",
    "description": "chrome extension for hackcmu",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    }, 
    "permissions": ["storage", "activeTab", "contextMenus"], 
    "action": {
        "default_popup": "popup.html"
    }, 
    "content_scripts": [
        {
            "matches": ["https://*.google.com/*"],
            "js": ["content.js"]
        }
    ]
  }